html, body {
	font-size: 15px
	font-family typeface-body
	color: color-black
}
p {
	font-size .86rem
}
.Unselectable {
	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;
}
p {
	line-height 150%
	margin -.5em 0 1em 0
	text-align left
	cursor: default
	font-size: .86rem;
}	
a {
	text-decoration: none
	color text-color		
}	
strong {
	font-weight normal
}
h1,h2,h3,h4,h5,h6 {
	margin 0 0
	padding: 0 0
	font-weight: normal
	line-height 100%
	position relative
	cursor: default;
	font-size: 1rem;
}	
.Display2 {
	font-size: 2rem;
	font-family: typeface-body-black;
	color: wjite;
}
.Display2.Black {
	color: black;
}
.Display1 {
	font-size: 2.26rem;
}
.Headline {
	font-size: 1.667rem;
	font-family: CircularStdBold;
}
.Title {
	font-size: 1.2rem;
}
.Subhead1 {
	font-size: 1rem;
	font-weight: bold;
}
.Caption {
	font-size: .6rem;
	font-family: typeface-body-bold;	
	letter-spacing: .08rem;
	text-transform: uppercase;
}
.CallToAction {
	font-size: .75rem;
	font-family: CircularStdBold;	
	letter-spacing: .08rem;
	text-transform: uppercase;
}
.TextWhite {
	color: color-white;
}








		