html, body
	font-size: 15px
	font-family typeface-body
	color: color-black

p
	font-size .86rem

.Unselectable
	-moz-user-select: none;
	-webkit-user-select: none;
	-ms-user-select: none;

p
	line-height 150%
	margin -.5em 0 1em 0
	text-align left
	cursor: default
	font-size: .86rem;
	
a
	text-decoration: none
	color text-color		
	
strong 
	font-weight normal

h1,h2,h3,h4,h5,h6
	margin 0 0
	padding: 0 0
	font-weight: normal
	line-height 100%
	position relative
	z-index 9999
	cursor: default;
	font-size: 1rem;
	i
		font-style normal
	
.Display2 {
	font-size: 3rem;
}
.Display1 {
	font-size: 2.26rem;
}
.Headline {
	font-size: 1.667rem;
	font-family: CircularStdBold;
}
.Title {
	font-size: 1.2rem;
}
.Subhead1 {
	font-size: 1rem;
	font-weight: bold;
}
.Caption2 {
	font-size: .73rem;
	font-family: CircularStdBold;	
	letter-spacing: .08rem;
	text-transform: uppercase;
}
.Caption {
	font-size: .8;
}
.CallToAction {
	font-size: .75rem;
	font-family: CircularStdBold;	
	letter-spacing: .08rem;
	text-transform: uppercase;
}
.TextWhite {
	color: color-white;
}
.mb1 {
	margin-bottom: 1rem;
}
.mb2 {
	margin-bottom: 2rem;
}
.Opa50 {
	opacity: .5;
}











		