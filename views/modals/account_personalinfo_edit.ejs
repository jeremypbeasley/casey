<% include ../partials/header %>
<div class="FeatureLabel NoNav pt5 pl2 pb2 Display2 Black">Personal Information</div>
<div class="ButtonClose" onclick="location.href='account_personalinfo_details';">Close</div>
<form class="FormPersInfo mr2 ml2">
	<div>
		<label for="firstname" class="op50 FormLabel">First Name</label>
		<input type="text" name="firstname" class="FormInput pl2" placeholder="Jeremy">
	</div>
	<div>
		<label for="lastname" class="op50 FormLabel">Last</label>
		<input type="text" name="lastname" class="FormInput pl2" placeholder="Beasley">
	</div>
	<div>
		<label for="email" class="op50 FormLabel">Email</label>
		<input type="text" name="email" class="FormInput pl2" placeholder="jeremy@bsley.com">
	</div>
	<div>
		<label for="phone" class="op50 FormLabel">Phone</label>
		<input type="text" name="phone" class="FormInput pl2" placeholder="(206) 390-9023">
	</div>
	<div>
		<label for="AddressLine1" class="op50 FormLabel">Address (Line 1)</label>
		<input type="text" name="AddressLine1" class="FormInput pl2" placeholder="892 24th Ave S">
	</div>
	<div>
		<label for="AddressLine2" class="op50 FormLabel">Address (Line 2)</label>
		<input type="text" name="AddressLine2" class="FormInput pl2" placeholder="Apt 12">
	</div>
	<div>
		<label for="city" class="op50 FormLabel">City</label>
		<input type="text" name="city" class="FormInput pl2" placeholder="Seattle">
	</div>
	<div>
		<label for="state" class="op50 FormLabel">State</label>
		<select name="state" class="FormInput FormDropdown pl1">
			<option value="" disabled selected class=op50>Washington</option>
			<option value="AL">Alabama</option>
